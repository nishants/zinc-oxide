.vocabset.card
  .assigned(ng-if='vocabset.assigned')
    .assigned-date Assigned on {{vocabset.assigned.date}}
    .assigned-by
      .picture
        img(ng-src='{{vocabset.assigned.by.picture}}')
      .name(ng-bind='vocabset.assigned.by.name')
    .assigned-message(ng-bind='vocabset.assigned.message')
  .cover-image
    img(ng-src='{{vocabset.image}}')