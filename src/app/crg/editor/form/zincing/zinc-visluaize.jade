.skill.zinc-visualize
  .crg-bar
    label Visualize
    button(ng-click='zincVisualizeEditor.add()') Add
  ul.undo-list
    li.skill-form(ng-repeat='visualize in editor.game.zincing.visualize')
      .skill-param
        label.param-label Focus
        input.param-input(type='text', disbaled='true', ng-model='visualize.focus.text')
      .skill-param
        label.param-label Phrase
        input.param-input(type='text', disbaled='true', ng-model='visualize.phrase.text')
      .skill-param
        label.param-label Transcript
        textarea(ng-model='visualize.transcript.text').param-input

      button(ng-click='editor.game.zincing.visualize.splice($index,1)').remove Remove
      button(ng-click='').remove Preview
