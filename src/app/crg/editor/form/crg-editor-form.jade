.crg-editor-form
  .scene-editor.card(ng-repeat='scene in editor.script.scenes', ng-init='sceneIndex = $index')
    .accordion-bar
      label {{$index +1}}. {{scene.label}}
      button(ng-click='editor.previewScene(scene)').remove Preview
      button(ng-click='editor.removeScene(scene)').remove Remove
    .scene-config-form
      .zinc-visualize(ng-if='scene.name == "zinc-visualize"')
        include ../../scenes/zinc-visualize/zinc-visluaize-editor
      .zinc-imagine(ng-if='scene.name == "zinc-imagine"')
        include ../../scenes/zinc-imagine/zinc-imagine-editor
      .find-all-key-images(ng-if='scene.name == "find-all-key-images"')
        include ../../scenes/zinc-key-images/find-all-key-images